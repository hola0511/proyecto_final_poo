class Usuario:
  def __init__(self, id, nombre, correo):
    self.id = id
    self.nombre = nombre
    self.correo = correo
    self.generos_interes = []

  def elegirGenero(self, genero):
    self.generos_interes.append(genero)

  def calificar(self, pelicula_serie, puntuacion):
    recomendacion = Recomendacion(self, pelicula_serie, puntuacion)
    return recomendacion


class Genero:
  def __init__(self, id, nombre):
    self.id = id
    self.nombre = nombre

