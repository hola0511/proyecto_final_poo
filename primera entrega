    pass


 class PeliculaSerie:
  def __init__(self, id, titulo, generos):
    self.id = id
    self.titulo = titulo
    self.generos = generos
    self.puntuacion = 0.0

  def obtenerTitulo(self):
    return self.titulo

  def obtenerGeneros(self):
    return self.generos

  def obtenerPuntuacion(self):
    return self.puntuacion


class Recomendacion:
  def __init__(self, usuario, pelicula_serie, puntuacion):
    self.usuario = usuario
    self.pelicula_serie = pelicula_serie
    self.puntuacion = puntuacion

  def obtenerUsuario(self):
     return self.usuario

  def obtenerPelicula(self):
     return self.pelicula_serie

  def obtenerPuntuacion(self):
     return self.puntuacion
